{
    "name": "max-lit/oauth2-vkontakte",
    "description": "Vkontakte provider for league/oauth2-client",
    "keywords": [
        "league",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Litvyakov",
            "email": "litvy-maksim@yandex.ru"
        }
    ],
    "require": {
